{
  "esp32_can.canbus": {
    "schemas": {
      "CONFIG_SCHEMA": {
        "type": "schema",
        "schema": {
          "extends": [
            "canbus.CANBUS_SCHEMA"
          ],
          "config_vars": {
            "id": {
              "id_type": {
                "class": "esp32_can::ESP32Can",
                "parents": [
                  "canbus::CanbusComponent",
                  "Component"
                ]
              }
            },
            "bit_rate": {},
            "rx_pin": {
              "key": "Required",
              "internal": true,
              "modes": [
                "input"
              ],
              "type": "pin",
              "docs": "**[Pin](https://esphome.io/guides/configuration-types.html#config-pin)**: Receive pin.\n\n*See also: [ESP32 CAN](https://esphome.io/components/canbus/esp32_can.html#configuration-variables)*"
            },
            "tx_pin": {
              "key": "Required",
              "internal": true,
              "modes": [
                "output"
              ],
              "type": "pin",
              "docs": "**[Pin](https://esphome.io/guides/configuration-types.html#config-pin)**: Transmit pin.\n\n*See also: [ESP32 CAN](https://esphome.io/components/canbus/esp32_can.html#configuration-variables)*"
            },
            "rx_queue_len": {
              "key": "Optional",
              "type": "integer",
              "data_type": "uint32_t",
              "docs": "**int**: Length of RX queue.\n\n*See also: [ESP32 CAN](https://esphome.io/components/canbus/esp32_can.html#configuration-variables)*"
            },
            "tx_queue_len": {
              "key": "Optional",
              "type": "integer",
              "data_type": "uint32_t",
              "docs": "**int**: Length of TX queue, 0 to disable.\n\n*See also: [ESP32 CAN](https://esphome.io/components/canbus/esp32_can.html#configuration-variables)*"
            },
            "tx_enqueue_timeout": {
              "key": "Optional",
              "type": "schema",
              "schema": {
                "extends": [
                  "core.positive_time_period_milliseconds"
                ]
              },
              "docs": "**[Time](https://esphome.io/guides/configuration-types.html#config-time)**: Maximum time to wait when the TX queue is full before dropping the message (by default, this is set to the time it takes to send 10 CAN messages at the given bit rate).\n\n*See also: [ESP32 CAN](https://esphome.io/components/canbus/esp32_can.html#configuration-variables)*"
            }
          }
        }
      }
    }
  }
}