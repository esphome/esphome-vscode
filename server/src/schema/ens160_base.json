{
  "ens160_base": {
    "schemas": {
      "CONFIG_SCHEMA_BASE": {
        "type": "schema",
        "schema": {
          "config_vars": {
            "eco2": {
              "key": "Required",
              "type": "schema",
              "schema": {
                "extends": [
                  "sensor.SENSOR_SCHEMA"
                ],
                "config_vars": {
                  "unit_of_measurement": {
                    "default": "ppm"
                  },
                  "icon": {
                    "default": "mdi:molecule-co2"
                  },
                  "accuracy_decimals": {
                    "default": "0"
                  },
                  "device_class": {
                    "default": "carbon_dioxide"
                  },
                  "state_class": {
                    "default": "measurement"
                  }
                }
              }
            },
            "tvoc": {
              "key": "Required",
              "type": "schema",
              "schema": {
                "extends": [
                  "sensor.SENSOR_SCHEMA"
                ],
                "config_vars": {
                  "unit_of_measurement": {
                    "default": "ppb"
                  },
                  "icon": {
                    "default": "mdi:radiator"
                  },
                  "accuracy_decimals": {
                    "default": "0"
                  },
                  "device_class": {
                    "default": "volatile_organic_compounds_parts"
                  },
                  "state_class": {
                    "default": "measurement"
                  }
                }
              }
            },
            "aqi": {
              "key": "Required",
              "type": "schema",
              "schema": {
                "extends": [
                  "sensor.SENSOR_SCHEMA"
                ],
                "config_vars": {
                  "icon": {
                    "default": "mdi:chemical-weapon"
                  },
                  "accuracy_decimals": {
                    "default": "0"
                  },
                  "device_class": {
                    "default": "aqi"
                  },
                  "state_class": {
                    "default": "measurement"
                  }
                }
              }
            },
            "compensation": {
              "key": "Optional",
              "type": "schema",
              "schema": {
                "config_vars": {
                  "temperature": {
                    "key": "Required",
                    "use_id_type": "sensor::Sensor",
                    "type": "use_id"
                  },
                  "humidity": {
                    "key": "Required",
                    "use_id_type": "sensor::Sensor",
                    "type": "use_id"
                  }
                }
              }
            },
            "update_interval": {
              "key": "Optional",
              "default": "60s"
            }
          },
          "extends": [
            "core.COMPONENT_SCHEMA"
          ]
        }
      }
    }
  }
}