{
  "interval": {
    "schemas": {
      "CONFIG_SCHEMA": {
        "type": "trigger",
        "schema": {
          "config_vars": {
            "id": {
              "key": "GeneratedID",
              "id_type": {
                "class": "interval::IntervalTrigger",
                "parents": [
                  "Trigger<>",
                  "Trigger",
                  "PollingComponent",
                  "Component"
                ]
              }
            },
            "startup_delay": {
              "key": "Optional",
              "default": "0s",
              "type": "schema",
              "schema": {
                "extends": [
                  "core.positive_time_period_milliseconds"
                ]
              },
              "docs": "**[Time](https://esphome.io/guides/configuration-types.html#config-time)**: An optional startup delay - defaults to zero.\n\n*See also: [Interval Component](https://esphome.io/components/interval.html#configuration-variables)*"
            },
            "interval": {
              "key": "Required",
              "type": "schema",
              "schema": {
                "extends": [
                  "core.positive_time_period_milliseconds"
                ]
              },
              "docs": "**[Time](https://esphome.io/guides/configuration-types.html#config-time)**: The interval to execute the action with.\n\n*See also: [Interval Component](https://esphome.io/components/interval.html#configuration-variables)*"
            },
            "setup_priority": {
              "key": "Optional"
            },
            "then": {
              "type": "trigger",
              "docs": "**[Action](https://esphome.io/automations/actions.html#config-action)**: The action to perform.\n\n*See also: [Interval Component](https://esphome.io/components/interval.html#configuration-variables)*"
            }
          }
        },
        "has_required_var": true
      }
    }
  }
}