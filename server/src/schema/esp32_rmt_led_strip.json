{
  "esp32_rmt_led_strip.light": {
    "schemas": {
      "CONFIG_SCHEMA": {
        "type": "schema",
        "schema": {
          "extends": [
            "light.ADDRESSABLE_LIGHT_SCHEMA",
            "core.COMPONENT_SCHEMA"
          ],
          "config_vars": {
            "output_id": {
              "key": "GeneratedID",
              "id_type": {
                "class": "esp32_rmt_led_strip::ESP32RMTLEDStripLightOutput",
                "parents": [
                  "light::AddressableLight",
                  "light::LightOutput",
                  "Component"
                ]
              }
            },
            "pin": {
              "key": "Required",
              "internal": true,
              "modes": [
                "output"
              ],
              "type": "pin"
            },
            "num_leds": {
              "key": "Required",
              "type": "integer",
              "data_type": "positive_not_null_int"
            },
            "rgb_order": {
              "key": "Required",
              "type": "enum",
              "values": {
                "RGB": null,
                "RBG": null,
                "GRB": null,
                "GBR": null,
                "BGR": null,
                "BRG": null
              }
            },
            "rmt_channel": {
              "key": "Optional"
            },
            "rmt_symbols": {
              "key": "Optional",
              "type": "integer"
            },
            "max_refresh_rate": {
              "key": "Optional",
              "type": "schema",
              "schema": {
                "extends": [
                  "core.positive_time_period_microseconds"
                ]
              }
            },
            "chipset": {
              "key": "Optional",
              "type": "enum",
              "values": {
                "WS2811": null,
                "WS2812": null,
                "SK6812": null,
                "APA106": null,
                "SM16703": null
              }
            },
            "is_rgbw": {
              "key": "Optional",
              "default": "False",
              "type": "boolean"
            },
            "is_wrgb": {
              "key": "Optional",
              "default": "False",
              "type": "boolean"
            },
            "use_dma": {
              "key": "Optional",
              "type": "boolean"
            },
            "use_psram": {
              "key": "Optional",
              "default": "True",
              "type": "boolean"
            },
            "bit0_high": {
              "key": "Optional",
              "type": "schema",
              "schema": {
                "extends": [
                  "core.positive_time_period_nanoseconds"
                ]
              }
            },
            "bit0_low": {
              "key": "Optional",
              "type": "schema",
              "schema": {
                "extends": [
                  "core.positive_time_period_nanoseconds"
                ]
              }
            },
            "bit1_high": {
              "key": "Optional",
              "type": "schema",
              "schema": {
                "extends": [
                  "core.positive_time_period_nanoseconds"
                ]
              }
            },
            "bit1_low": {
              "key": "Optional",
              "type": "schema",
              "schema": {
                "extends": [
                  "core.positive_time_period_nanoseconds"
                ]
              }
            },
            "reset_high": {
              "key": "Optional",
              "default": "0 us",
              "type": "schema",
              "schema": {
                "extends": [
                  "core.positive_time_period_nanoseconds"
                ]
              }
            },
            "reset_low": {
              "key": "Optional",
              "default": "0 us",
              "type": "schema",
              "schema": {
                "extends": [
                  "core.positive_time_period_nanoseconds"
                ]
              }
            }
          }
        }
      }
    }
  }
}