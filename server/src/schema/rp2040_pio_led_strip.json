{
  "rp2040_pio_led_strip.light": {
    "schemas": {
      "CONFIG_SCHEMA": {
        "type": "schema",
        "schema": {
          "extends": [
            "light.ADDRESSABLE_LIGHT_SCHEMA"
          ],
          "config_vars": {
            "output_id": {
              "key": "GeneratedID",
              "id_type": {
                "class": "rp2040_pio_led_strip::RP2040PIOLEDStripLightOutput",
                "parents": [
                  "light::AddressableLight",
                  "light::LightOutput",
                  "Component"
                ]
              }
            },
            "pin": {
              "key": "Required",
              "internal": true,
              "modes": [
                "output"
              ],
              "type": "pin"
            },
            "num_leds": {
              "key": "Required",
              "type": "integer",
              "data_type": "positive_not_null_int"
            },
            "rgb_order": {
              "key": "Required",
              "type": "enum",
              "values": {
                "RGB": null,
                "RBG": null,
                "GRB": null,
                "GBR": null,
                "BGR": null,
                "BRG": null
              }
            },
            "pio": {
              "key": "Required",
              "type": "enum",
              "values": {
                "0": null,
                "1": null
              }
            },
            "chipset": {
              "key": "Optional",
              "type": "enum",
              "values": {
                "WS2812": null,
                "WS2812B": null,
                "SK6812": null,
                "SM16703": null,
                "CUSTOM": null
              }
            },
            "is_rgbw": {
              "key": "Optional",
              "default": "False",
              "type": "boolean"
            },
            "bit0_high": {
              "key": "Optional"
            },
            "bit0_low": {
              "key": "Optional"
            },
            "bit1_high": {
              "key": "Optional"
            },
            "bit1_low": {
              "key": "Optional"
            }
          }
        }
      }
    }
  }
}